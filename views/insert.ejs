<div class="container">
    <table class="table table-striped table-dark table-hover w-75">
        <thead>
          <tr>
            <% for(var i=0 ; i < header.length ; i++) {%>
              <th scope="col">
                <%= header[i] %>
              </th>
              <% } %>
  
          </tr>
        </thead>
        <tbody>
            <tr>
              <% for(var j=0 ; j <=list.length ; j++) {%>
                <td class="text-success">
                  <%=list[j]%>
                </td>
                <% } %>
            </tr>
        </tbody>
      </table>
</div>